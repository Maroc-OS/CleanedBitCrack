<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
	  <CUDA_INCLUDE>$(CUDA_PATH)\include</CUDA_INCLUDE>
	  <CUDA_LIB>$(CUDA_PATH)\lib\x64</CUDA_LIB>
    <OPENCL_INCLUDE>$(CUDA_PATH)\include</OPENCL_INCLUDE>
    <OPENCL_LIB>$(CUDA_PATH)\lib\x64</OPENCL_LIB>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="OPENCL_INCLUDE">
      <Value>$(OPENCL_INCLUDE)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="OPENCL_LIB">
      <Value>$(OPENCL_LIB)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
	    <BuildMacro Include="CUDA_INCLUDE">
      <Value>$(CUDA_INCLUDE)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="CUDA_LIB">
      <Value>$(CUDA_LIB)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>